//Estrutura para Alarme baseada no cruzamento MM9

begin
  Se (IsBought) ent達o
    Inicio
      Se(Close<= Minima[1]) ent達o
        Alert(clYellow);
      Fim
  Sen達o
    Inicio
      Se (MediaExp(9,Close) > MediaExp(9,Close)[1]) 
        e (MediaExp(9,Close)[1]< MediaExp(9,Close)[2]) ent達o //pivot
        Alert(clGreen);
  Fim;
end;